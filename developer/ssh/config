# Default properties
AddKeysToAgent yes
UseKeychain yes
IdentitiesOnly yes

# Default host
Host *
	IdentityFile ~/.ssh/id_rsa

# Key for example.com
Host example.com
  	HostName example.com
  	User git
  	IdentityFile ~/.ssh/id_rsa
  	
# Add more Host sections similar to the one above as needed for other identities
